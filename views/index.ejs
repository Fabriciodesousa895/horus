      <% result.forEach((result, index) => { %>
        <% if (index % 3 === 0) { %>
          <div class="row">
        <% } %>
      
        <div class="col-md-4">
            <div class="card mt-3" style="width: 600px;height: 450px;">
              <div class="card-header" style="color: <%= result.COR %>;display: flex;justify-content: space-around;align-items: center; " >
                <p><%= result.TITULO %></p> <p>Postagem <%= result.POSTAGEM %></p> 
              </div>
              <div class="card-body">
                <p><%= result.MENSAGEM %></p>
    
              </div>
              <div class="card-footer" style="display: flex;justify-content: space-around;align-items: center;">
                <p>Data inicio: <%= result.INICIO %></p>
                <p>Data fim: <%= result.FIM %></p>
                
              </div>
    
      
          </div>
        </div>
      
        <% if ((index + 1) % 3 === 0 || index === result.length - 1) { %>
          </div>
        <% } %>
      <% }) %>
      
    


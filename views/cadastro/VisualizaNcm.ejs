<%-include('../partials/head')%>
<div class="" style="display: flex;height: auto;padding: 30px;">
    <form action="">
<!--  -->
    <div class="row">
        <%result.forEach(result=>{%>

                <div class="col-sm-7">
                    <span>Código Ncm</span>
                <input type="text" placeholder="Código Ncm" id="COD_NCM_NEW" value="<%=result.COD%>" class="form-control">
                <input type="hidden" placeholder="Código Ncm" id="COD_NCM_OLD" value="<%=result.COD%>" class="form-control">
               
                <span>Descrição</span>
                
                <textarea  id="NCM_DESC" placeholder="Descrição do Ncm" class="form-control" cols="30" rows="10"><%=result.NCM_DESC%></textarea>
                </div>

               <div class="col-sm-5">
                <span>Usuário inclusão</span>
                <input type="text" disabled class="form-control" value="<%=result.USU_INCLUSAO%>">

                <span>Data/hora inclusao</span>
                <input type="text" disabled class="form-control" value="<%=result.DT_INCLU%>">

                <span>Usuário alteração</span>
                <input type="text" disabled class="form-control" value="<%=result.USU_ALTER%>">
                  
                <span>Data/hora alteração</span>
                <input type="text" disabled class="form-control" value="<%=result.DT_ALTER%>">

                </div>
        <%})%>

    </div>

</form>
    
</div>

<script type="module" src="/js/cadastro/VisualizaNcm.js"></script>
<%-include('../partials/footer')%>

                <label for="FORNECEDOR">Somente fornecedor?</label>
                <input type="checkbox" id="FORNECEDOR">
                <br>
                <label for="BLOQUEADOS">Venda bloqueada?</label>
                <input type="checkbox" id="BLOQUEADOS">
                <br>
                <button id="Buscar" type="submit" class="btn btn-success">Buscar</button>
                <button id="TOTAL" class="btn btn-success ">00</button>


            </div>
            <div style="width: 100%;">
                <table class="table table-hover">
                    <thead>
                        <th>ID</th>
                        <th>Nome</th>
                        <th>CPF/CNPJ</th>
                        <th>IE_RG</th>
                        <th>Email</th>
                    </thead>
                    <tbody id="tbody">

                    </tbody>
                </table>
            </div>
        </div>

<%- include('../partials/head'); %>

    <%- include('../partials/header'); %>
        <% let CAMPOS=Objeto.CAMPOS %>
            <% let CONSTRAINT=Objeto.CONSTRAINT %>
                <% let TABELA_NOME=Objeto.TABELA_NOME %>
                    <% let INDEXES=Objeto.INDEXES %>
                    <%if(CAMPOS.length==0){%>
                        <h5>Não há registros para visualizar.</h5>
                        <%}else{%>
                            <h3>Campos da tabela <%=TABELA_NOME[0]%>
                            </h3>
                            <table class="table mt-3 table-hover" style="min-width: 800;width: 900px;">
                                <thead>
                                    <tr>
                                        <th>CAMPO</th>
                                        <th>Tipo</th>
                                        <th>NULL</th>
                                        <th>Tamanho</th>
                                        <th>Default</th>
                                        <th>Descrição</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (var i=0; i < CAMPOS.length; i++) { %>
                                        <tr>
    
                                            <td>
                                                <%= CAMPOS[i][0] %>
                                            </td>
                                            <td>
                                                <%= CAMPOS[i][1] %>
                                            </td>
                                            <td>
                                                <%= CAMPOS[i][2] %>
                                            </td>
                                            <td>
                                                <%= CAMPOS[i][3] %>
                                            </td>
                                            <td>
                                                <%= CAMPOS[i][4] %>
                                            </td>
                                            <td>
                                                <%= CAMPOS[i][5] %>
                                            </td>
                                        </tr>
    
                                        <% } %>
    
                                </tbody>
                            </table>



                            <!--     -->
                        <%if(CONSTRAINT.length==0){%>
                            <h5>Não há Constraints para visualizar.</h5>
                            <%}else{%>
                                <h3>Constraints relacionados a tabela <%=TABELA_NOME[0]%>
                                </h3>

                                <table class="table mt-3 table-hover" style="min-width: 500;width: 600px;" <thead>
                                    <tr>
                                        <th>CAMPO</th>
                                        <th>NOME</th>
                                        <th>POSICAO</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                        <% for (var i=0; i < CONSTRAINT.length; i++) { %>
                                            <tr>

                                                <td>
                                                    <%= CONSTRAINT[i][0] %>
                                                </td>
                                                <td>
                                                    <%= CONSTRAINT[i][1] %>
                                                </td>
                                                <td>
                                                    <%= CONSTRAINT[i][2] %>
                                                </td>

                                            </tr>

                                            <% } %>

                                    </tbody>
                                </table>
                                <%}%>

                                    <%if(INDEXES.length==0){%>
                                        <h5>Não há indices para visualizar.</h5>
                                        <%}else{%>
                                            <h3> Indices relacionados a tabela <%=TABELA_NOME[0]%>
                                            </h3>
                                            <table class="table mt-3 table-hover" style="min-width: 500;width: 600px;" <thead>
                                                <tr>
                                                    <th>INDEX_NAME</th>
                                                    <th>INDEX_TYPE </th>
                                                    <th>CONSTRAINT_INDEX</th>
                                                    <th>NUM_ROWS</th>

                                                </tr>
                                                </thead>
                                                <tbody>
                                                    <% for (var i=0; i < INDEXES.length; i++) { %>
                                                        <tr>

                                                            <td>
                                                                <%= INDEXES[i][0] %>
                                                            </td>
                                                            <td>
                                                                <%= INDEXES[i][1] %>
                                                            </td>
                                                            <td>
                                                                <%= INDEXES[i][2] %>
                                                            </td>
                                                            <td>
                                                                <%= INDEXES[i][3] %>
                                                            </td>

                                                        </tr>

                                                        <% } %>

                                                </tbody>
                                            </table>
                                            <%}%>




                            <%}%>

                        

                                                <%- include('../partials/footer'); %>